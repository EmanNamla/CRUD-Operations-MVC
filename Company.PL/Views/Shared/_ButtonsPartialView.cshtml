@model string
<td>
    <a asp-action="Details" class="btn btn-info"  asp-route-id="@Model">
        <i class="fas fa-eye"></i>
    </a>
</td>
<td>
    <a asp-action="Edit" class="btn btn-warning" asp-route-id="@Model">
        <i class="fas fa-pencil"></i>
    </a>
</td>
<td>
    <a asp-action="Delete" class="btn btn-danger" asp-route-id="@Model">
        <i class="fas fa-trash"></i>
    </a>
</td>
